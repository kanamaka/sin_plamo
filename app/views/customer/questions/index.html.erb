<header>
 <%= render 'layouts/header' %>
</header>
 <div class="row mt-5" style="width: 100%; margin-bottom:300px">
  <div style="width: 800px; margin:0 auto;">
   <h4>質問一覧</h4>
   <tr>
    <% @questions.each do |question| %>
    <td>
     <%= @question.title %>
    </td>
    <td>
     <%= @question.content %>
    </td>
    <% end %>
   </tr>
  </div>
 </div>
<footer>
 <%= render 'layouts/footer' %>
</footer>